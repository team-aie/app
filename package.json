{
  "name": "aie",
  "version": "0.3.0",
  "description": "A cross-platform audio recorder designed for recording using recording lists (a.k.a. reclists).",
  "private": true,
  "scripts": {
    "start": "electron-webpack dev",
    "postinstall": "npm run generate-licenses-ts",
    "clean": "rm -rf dist coverage",
    "compile": "electron-webpack",
    "generate-licenses-ts": "webpack --config webpack.license.config.js",
    "dist": "electron-builder -mwl",
    "dist:macos": "electron-builder -m",
    "dist:win": "electron-builder -w",
    "dist:linux": "electron-builder -l",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --ext .js,.ts,.tsx",
    "lint:css": "stylelint src",
    "test": "npm run test:unit:ci",
    "test:unit": "ELECTRON_RUN_AS_NODE=true electron $(which jest)",
    "test:unit:ci": "ELECTRON_RUN_AS_NODE=true electron $(which jest) --ci --collectCoverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-aie/app.git"
  },
  "keywords": [],
  "author": "team-aie",
  "contributors": [
    {
      "name": "Iluminar"
    },
    {
      "name": "Xinxex"
    },
    {
      "name": "Kar Ash"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/team-aie/app/issues"
  },
  "homepage": "https://github.com/team-aie/app#readme",
  "dependencies": {
    "audiobuffer-to-wav": "^1.0.0",
    "bootstrap": "^4.4.1",
    "chardet": "^1.1.0",
    "dexie": "^3.0.1",
    "electron-log": "^4.1.2",
    "electron-updater": "^4.3.1",
    "i18next": "^19.4.4",
    "i18next-browser-languagedetector": "^4.1.1",
    "i18next-fs-backend": "^1.0.2",
    "node-source-han-sans": "^1.1.0-2.001R",
    "node-source-han-sans-sc": "^1.1.0-2.001R",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.4.0",
    "react-transition-group": "^4.4.1",
    "react-use": "^14.2.0",
    "sanitize-filename": "^1.6.3",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@types/audiobuffer-to-wav": "^1.0.0",
    "@types/dom-mediacapture-record": "^1.0.5",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.12.35",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-test-renderer": "^16.9.2",
    "@types/react-transition-group": "^4.2.4",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-webpack": "^2.8.2",
    "electron-webpack-eslint": "^6.0.0",
    "electron-webpack-ts": "^4.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^26.0.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "semantic-release": "^17.0.7",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-prettier": "^1.1.2",
    "text-decoding": "^1.0.0",
    "ts-jest": "^25.5.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-license-plugin": "^4.1.1"
  }
}
