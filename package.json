{
  "name": "aie",
  "version": "0.11.1",
  "description": "A cross-platform audio recorder designed for recording using recording lists (a.k.a. reclists).",
  "private": true,
  "scripts": {
    "start": "electron-webpack dev",
    "postinstall": "npm-run-all postinstall:*",
    "clean": "rm -rf dist coverage",
    "compile": "electron-webpack",
    "postinstall:husky": "husky install",
    "postinstall:generate-licenses-ts": "webpack --config webpack.license.config.js",
    "dist": "electron-builder -mwl",
    "dist:macos": "electron-builder -m",
    "dist:win": "electron-builder -w",
    "dist:linux": "electron-builder -l",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint . --ext .js,.ts,.tsx",
    "lint:css": "stylelint src",
    "lint:md": "prettier -c **/*.md",
    "fix-lint": "npm-run-all fix-lint:*",
    "fix-lint:js": "eslint . --ext .js,.ts,.tsx --fix",
    "fix-lint:css": "stylelint src --fix",
    "fix-lint:md": "prettier -w **/*.md",
    "test": "npm run test:unit:ci",
    "test:unit": "ELECTRON_RUN_AS_NODE=true electron $(which jest)",
    "test:unit:ci": "ELECTRON_RUN_AS_NODE=true electron $(which jest) --ci --collectCoverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-aie/app.git"
  },
  "keywords": [],
  "author": "team-aie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/team-aie/app/issues"
  },
  "homepage": "https://github.com/team-aie/app#readme",
  "dependencies": {
    "audiobuffer-to-wav": "^1.0.0",
    "bootstrap": "^4.5.0",
    "chardet": "^1.2.1",
    "chokidar": "^3.5.1",
    "delta_eng_ver5": "file:external/delta_eng_ver5",
    "dexie": "^3.0.1",
    "electron-log": "^4.2.2",
    "electron-updater": "^4.3.8",
    "i18next": "^19.8.9",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-fs-backend": "^1.0.7",
    "react": "^17.0.1",
    "react-bootstrap": "^1.1.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.7",
    "react-transition-group": "^4.4.1",
    "react-use": "^17.1.1",
    "rxjs": "^6.5.5",
    "sanitize-filename": "^1.6.3",
    "source-map-support": "^0.5.19",
    "wavesurfer.js": "^4.5.0",
    "z.cvvc_normal": "file:external/z.cvvc_normal"
